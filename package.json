{
  "name": "@henrist/cdk-lambda-config",
  "version": "0.0.0-development",
  "description": "CDK Construct for adding config.json file to a Lambda Function",
  "repository": {
    "type": "git",
    "url": "https://github.com/henrist/cdk-lambda-config"
  },
  "scripts": {
    "build": "rimraf dist && webpack && tsc",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "npm run build",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "cdk",
    "lambda",
    "config"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "dist/**/*",
    "lib/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.51.0",
    "@aws-cdk/aws-iam": "1.51.0",
    "@aws-cdk/aws-lambda": "1.51.0",
    "@aws-cdk/core": "1.51.0",
    "@aws-cdk/custom-resources": "1.51.0",
    "@commitlint/cli": "9.1.1",
    "@commitlint/config-conventional": "9.1.1",
    "@types/adm-zip": "0.4.33",
    "@types/aws-lambda": "8.10.59",
    "@types/jest": "26.0.4",
    "@types/node": "13.13.14",
    "@typescript-eslint/eslint-plugin": "3.6.1",
    "@typescript-eslint/parser": "3.6.1",
    "adm-zip": "0.4.16",
    "aws-sdk": "2.713.0",
    "axios": "0.19.2",
    "eslint": "7.4.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-deprecation": "1.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.2.5",
    "jest": "26.1.0",
    "jest-cdk-snapshot": "1.3.0",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "semantic-release": "17.1.1",
    "ts-jest": "26.1.2",
    "ts-loader": "8.0.0",
    "ts-node": "8.10.2",
    "typescript": "3.9.6",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "peerDependencies": {
    "@aws-cdk/aws-iam": "^1.50.0",
    "@aws-cdk/aws-lambda": "^1.50.0",
    "@aws-cdk/core": "^1.50.0",
    "@aws-cdk/custom-resources": "^1.50.0"
  }
}
